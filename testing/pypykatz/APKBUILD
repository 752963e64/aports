# Maintainer: Jonas <3426-spameier@users.gitlab.alpinelinux.org>
pkgname=pypykatz
pkgver=0.6.5
pkgrel=0
pkgdesc="Python implementation of Mimikatz"
url="https://github.com/skelsec/pypykatz"
arch="noarch"
license="MIT"
depends="
	py3-aesedb
	py3-aiosmb
	py3-aiowinreg
	py3-minidump
	py3-minikerberos
	py3-msldap
	py3-tqdm
	py3-unicrypto
	py3-winacl
	python3
	"
makedepends="
	py3-gpep517
	py3-wheel
	py3-setuptools
	"
source="$pkgname-$pkgver.tar.gz::https://github.com/skelsec/pypykatz/archive/refs/tags/$pkgver.tar.gz"
options="!check" # no tests provided

build() {
	gpep517 build-wheel \
		--wheel-dir dist \
		--output-fd 3 3>&1 >&2
}

package() {
	python3 -m installer -d "$pkgdir" dist/*.whl
}

sha512sums="
e45f224c1a100cc8c5e5f18146a9b678c4b745702e6347ca7f660976d8bc06168e39fe05668848857c70ea4cb7eea7a88a1f99e09ee1c71e44f347049673c044  pypykatz-0.6.5.tar.gz
"

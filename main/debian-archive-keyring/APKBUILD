pkgname=debian-archive-keyring
pkgver=2021.1
pkgrel=0
pkgdesc="GnuPG archive keys of the Debian archive"
url="https://packages.debian.org/sid/debian-archive-keyring"
arch="noarch"
license="GPL-2.0-or-later"
depends="gnupg"
options="!check" # upstream doesn't have a test suite.
subpackages="$pkgname-doc"
source="https://deb.debian.org/debian/pool/main/d/debian-archive-keyring/debian-archive-keyring_${pkgver}_all.deb"

unpack() {
	cd "$srcdir"
	for i in $source; do
		case $i in
			*.deb) ar x ${i##*/} ;;
		esac
	done
}

build() {
	return 0
}

package() {
	mkdir -p "$pkgdir"
	unxz -c "$srcdir"/data.tar.xz | tar -x -C "$pkgdir"/
}

sha512sums="20aeb3460bdfaa47aad0c069c5d39a2e385b0394d39db9ea76ad5a7e6fa6421c8603eb2241d2cf4ba82754291097ae416b1ec2a04c16a6065c5f0d04eb8d8946  debian-archive-keyring_2021.1_all.deb"

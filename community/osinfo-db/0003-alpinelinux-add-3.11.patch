From c92b2d4c5bb4aa5b15356d91270a921df89413dd Mon Sep 17 00:00:00 2001
From: Natanael Copa <ncopa@alpinelinux.org>
Date: Tue, 5 Jan 2021 09:36:46 +0100
Subject: [PATCH 3/5] alpinelinux: add 3.11

Signed-off-by: Natanael Copa <ncopa@alpinelinux.org>
---
 .../alpinelinux.org/alpinelinux-3.11.xml.in   | 108 ++++++++++++++++++
 1 file changed, 108 insertions(+)
 create mode 100644 data/os/alpinelinux.org/alpinelinux-3.11.xml.in

diff --git a/data/os/alpinelinux.org/alpinelinux-3.11.xml.in b/data/os/alpinelinux.org/alpinelinux-3.11.xml.in
new file mode 100644
index 00000000..c318a5d1
--- /dev/null
+++ b/data/os/alpinelinux.org/alpinelinux-3.11.xml.in
@@ -0,0 +1,108 @@
+<?xml version="1.0" encoding="UTF-8"?>
+<libosinfo version="0.0.1">
+<!-- Licensed under the GNU General Public License version 2 or later.
+     See http://www.gnu.org/licenses/ for a copy of the license text -->
+  <os id="http://alpinelinux.org/alpinelinux/3.11">
+    <short-id>alpinelinux3.11</short-id>
+    <name>Alpine Linux 3.11</name>
+    <version>3.11</version>
+    <vendor>Alpine Linux Project</vendor>
+    <family>linux</family>
+    <distro>alpinelinux</distro>
+    <upgrades id="http://alpinelinux.org/alpinelinux/3.10"/>
+    <derives-from id="http://alpinelinux.org/alpinelinux/3.10"/>
+
+    <release-date>2019-12-29</release-date>
+    <eol-date>2021-11-01</eol-date>
+
+    <resources arch="all">
+      <minimum>
+        <cpu>1000000000</cpu>
+        <n-cpus>1</n-cpus>
+        <ram>134217728</ram>
+        <storage>1073741824</storage>
+      </minimum>
+      <recommended>
+        <cpu>1000000000</cpu>
+        <n-cpus>1</n-cpus>
+        <ram>805306368</ram>
+        <storage>4294967296</storage>
+      </recommended>
+    </resources>
+
+    <variant id="standard">
+      <name>Alpinelinux 3.11 Standard</name>
+    </variant>
+    <variant id="extended">
+      <name>Alpinelinux 3.11 Extended</name>
+    </variant>
+    <variant id="virtual">
+      <name>Alpinelinux 3.11 Virtual</name>
+    </variant>
+    <variant id="xen">
+      <name>Alpinelinux 3.11 Xen</name>
+    </variant>
+
+    <!-- Standard Installer !-->
+    <media arch="i686">
+      <variant id="standard"/>
+      <iso>
+        <volume-id>alpine-std 3.11.\d x86$</volume-id>
+      </iso>
+    </media>
+    <media arch="x86_64">
+      <variant id="standard"/>
+      <iso>
+        <volume-id>alpine-std 3.11.\d x86_64$</volume-id>
+      </iso>
+    </media>
+    <media arch="ppc64le">
+      <variant id="standard"/>
+      <iso>
+        <volume-id>alpine-std 3.11.\d ppc64le$</volume-id>
+      </iso>
+    </media>
+    <media arch="s390x">
+      <variant id="standard"/>
+      <iso>
+        <volume-id>alpine-std 3.11.\d s390x$</volume-id>
+      </iso>
+    </media>
+
+    <!-- Extended Installer !-->
+    <media arch="i686">
+      <variant id="extended"/>
+      <iso>
+        <volume-id>alpine-ext 3.11.\d x86$</volume-id>
+      </iso>
+    </media>
+    <media arch="x86_64">
+      <variant id="extended"/>
+      <iso>
+        <volume-id>alpine-ext 3.11.\d x86_64$</volume-id>
+      </iso>
+    </media>
+
+    <!-- Virtual Installer !-->
+    <media arch="i686">
+      <variant id="virtual"/>
+      <iso>
+        <volume-id>alpine-virt 3.11.\d x86$</volume-id>
+      </iso>
+    </media>
+    <media arch="x86_64">
+      <variant id="virtual"/>
+      <iso>
+        <volume-id>alpine-virt 3.11.\d x86_64$</volume-id>
+      </iso>
+    </media>
+
+    <!-- Xen Installer !-->
+    <media arch="x86_64">
+      <variant id="xen"/>
+      <iso>
+        <volume-id>alpine-xen 3.11.\d x86_64$</volume-id>
+      </iso>
+    </media>
+   </os>
+ </libosinfo>
